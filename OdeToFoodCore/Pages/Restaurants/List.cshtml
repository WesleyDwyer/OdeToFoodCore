@page
@model OdeToFoodCore.Pages.Restuarants.ListModel
@{
}
    <h1>Restaurants</h1>
<table class="table table-striped">
    @foreach (var item in Model.Restaurants)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Location</td>
            <td>@item.Cuisine</td>
        </tr>
    }
</table>
@Model.Message